        <activity android:name=".module.home.view.mahasiswa.HomeActivity" />
        <activity android:name=".module.splashscreen.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".module.auth.mahasiswa.view.MainActivity" />
